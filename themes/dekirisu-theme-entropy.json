{
	"name": "Dekirisu's Rust Theme",
	"type": "dark",

	"semanticHighlighting": true,
	"semanticTokenColors": {

		/* ------------------------------- References ------------------------------- */
			"*.reference": {"italic": true},

		/* -------------------------------- Mutables -------------------------------- */
			"*.mutable": {"underline": true},

		/* ------------------------------- Secondaries ------------------------------ */
			"*.documentation": {"italic": true},
			"comment": "#5a5b5e",
			"brace": "#D867C0",
			"angle": "#D867C0",
			"parenthesis": "#D867C0",
			"namespace": {"foreground": "#B0BCCA"},

		// /* -------------------------------- Keywords -------------------------------- */
		// 	"keyword":"#b39265",

		/* ------------------------------- Arithmetics ------------------------------ */
			"arithmetic": {"foreground": "#D867C0"},
			"comparison": {"foreground": "#D867C0"},
			"keyword.controlFlow": {"foreground": "#D867C0"},

		/* -------------------------- Kinda Memory Related -------------------------- */
			"*.constant": {"foreground":"#DF7D3C"},
			"variable.static": {"foreground":"#DF7D3C"},
			"boolean": "#DF7D3C",
			"builtinType": {"foreground":"#DF7D3C"},
			"number": "#DF7D3C",
			"lifetime": {"foreground":"#B887A5"},
			"keyword.crateRoot": {"foreground": "#DF7D3C"},

		/* -------------------------- Further Generization -------------------------- */
			"macro":{"foreground": "#F3BE4A"},
			"macroBang":{"foreground": "#F3BE4A"},
			"typeParameter": {"foreground":"#F3BE4A"},

		/* -------------------------------- Variables ------------------------------- */
			"variable": "#7E87D8",
			"parameter": "#7E87D8",
			"property": "#7E87D8",

		/* --------------------------------- Struct --------------------------------- */
			"struct": {"foreground":"#C73A58"},
			"typeAlias": {"foreground":"#C73A58"},
		
		/* ---------------------------------- Enum ---------------------------------- */
			"enum": {"foreground":"#C73A58"},
			"enumMember": "#C73A58",

		/* ---------------------------------- Self ---------------------------------- */
			"selfKeyword": {"foreground": "#E067BC"},
			"selfTypeKeyword": {"foreground": "#E067BC"},

		/* -------------------------------- Functions ------------------------------- */
			"method": {"foreground": "#13CE9F", "italic":true},
			"function": {"foreground": "#13CE9F", "italic":true},

		/* ------------------------------ Asynchronous ------------------------------ */
			"*.async": {"foreground":"#F3BE4A"},
			"keyword.async": {"foreground":"#F3BE4A"},

		/* --------------------------------- Unsafe --------------------------------- */
			"*.unsafe": {"foreground":"#ff0000"},

		/* ---------------------------------- Trait --------------------------------- */
			"interface": {"foreground":"#C73A58"},

		/* ------------------------------- Proc Macros ------------------------------ */
            "*.attribute": {"foreground": "#6d5f41"}

	},
	"tokenColors": [

		/* -------------------------------- Mutables -------------------------------- */
			{"scope":[
				"storage.modifier.mut"
			],"settings": {
				"fontStyle": "underline"
			}},

		/* ------------------------------- Proc Macros ------------------------------ */
			{"scope":[
				"punctuation.brackets.attribute",
				"meta.attribute"
			],"settings": {
				"foreground": "#6d5f41"
			}},

		/* ------------------------------- Secondaries ------------------------------ */
			{"scope":[
				"comment",
				"punctuation",
				"keyword.operator.logical",
				"keyword.operator.namespace",
				"keyword.operator.assignment.equal",
				"keyword.operator.access",
				"keyword.operator.key-value",
				"keyword.operator.range"
			],"settings": {
				"foreground": "#5a5b5e"
			}},

        /* -------------------------------- Brackets -------------------------------- */
            {"scope":[
                "punctuation.brackets.curly",
                "punctuation.brackets.angle",
                "punctuation.brackets.round",
				"keyword.operator.arrow"
            ],"settings": {
                "foreground": "#D867C0"
            }},

		/* -------------------------------- Keywords -------------------------------- */
			{"scope":[
                "keyword.other.mut"
            ],"settings": {
				"foreground": "#b37f93"
			}},

		/* ------------------------------- Arithmetics ------------------------------ */
			{"scope":[
				"keyword.operator.comparison",
				"keyword.operator.math",
				"keyword.operator.assignment",
				"keyword.operator.question"
			],"settings": {
				"foreground": "#D867C0"
			}},
			{"scope":[
				"source"
                // "storage.type"
			],"settings": {
				"foreground": "#37C0F7"
			}},

		/* -------------------------- Kinda Memory Related -------------------------- */
			{"scope":[
				"entity.name.type.numeric",
				"entity.name.type.primitive",
				"variable.other.constant",
				"constant.other.caps"
			],"settings": {
				"foreground": "#DF7D3C"
			}},
			{"scope":[
				"constant.numeric.decimal",
				"constant.language"
			],"settings": {
				"foreground": "#DF7D3C"
			}},

		/* -------------------------------- Lifetime -------------------------------- */
            {"scope":[
                "entity.name.type.lifetime",
                "keyword.operator.borrow",
                "keyword.operator.dereference",
                "punctuation.definition.lifetime"
            ],"settings": {
                "foreground": "#B887A5"
            }},

		/* -------------------------- Further Generization -------------------------- */
			{"scope":[
				"entity.name.function.macro",
				"variable.other.metavariable.name",
				"keyword.operator.macro.dollar"
			],"settings": {
				"foreground": "#F3BE4A",
                "fontStyle": "italic"
			}},
			{"scope":"variable.other.metavariable.specifier","settings": {
				"foreground": "#7E87D8"
			}},

		/* -------------------------------- Variables ------------------------------- */
			{"scope":"variable.other","settings": {
				"foreground": "#7E87D8"
			}},

		/* --------------------------------- Struct --------------------------------- */
			{"scope":[
                "entity.name.type.struct",
                "keyword.declaration"
            ],"settings": {
				"foreground": "#C73A58"
			}},

		/* ---------------------------------- Self ---------------------------------- */
			{"scope":"variable.language.self","settings": {
				"foreground": "#A574CE",
                "fontStyle": "italic"
			}},

		/* ---------------------------------- Enum ---------------------------------- */
			{"scope":"entity.name.type.enum","settings": {
				"foreground": "#C73A58"
			}},
			{"scope":"variable.other.enummember","settings": {
				"foreground": "#C73A58"
			}},

		/* -------------------------------- Functions ------------------------------- */
			{"scope":[
				"meta.function.call",
				"meta.function.definition",
                "keyword.other.fn"
			],"settings": {
				"foreground": "#13CE9F",
                "fontStyle": "italic"
			}},

		/* --------------------------------- String --------------------------------- */
			{"scope":"string","settings": {
				"foreground": "#00A884"
			}},

            {"scope":"entity.name.namespace","settings": {
				"foreground": "#B0BCCA"
			}}
		/* ------------------------------------ - ----------------------------------- */

	],
